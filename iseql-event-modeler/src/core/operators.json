{
  "schemaVersion": "1.0.0",
  "parameterDefaults": {
    "zeta": "≤",
    "eta": "≤",
    "delta": "∞",
    "epsilon": "∞",
    "rho": 0
  },
  "notes": [
    "This catalog is used by Expert UI + Resolver/Validator/Serializer later.",
    "Allen 13 relations are represented as kind='alias' mapping to base ISEQL operators.",
    "Parameter meaning follows ISEQL literature (ζ, η comparators; δ/ε thresholds; ρ robustness slack)."
  ],
  "operators": [
    {
      "kind": "operator",
      "id": "bef",
      "code": "Bef",
      "label": "Before",
      "description": "Non-overlapping: left interval ends before right interval starts, within delta (threshold).",
      "arity": 2,
      "parameters": {
        "supported": ["delta", "rho", "zeta"],
        "defaults": { "delta": "∞", "rho": 0, "zeta": "≤" },
        "domains": {
          "delta": { "type": "threshold", "min": 0, "allowInfinity": true },
          "rho": { "type": "number", "min": 0 },
          "zeta": { "type": "comparator", "allowed": ["<", "≤", "=", "≥", ">"] }
        }
      },
      "constraintCompatibility": {
        "cardinality": true,
        "overlapPercentage": false,
        "robustness": true
      }
    },
    {
      "kind": "operator",
      "id": "aft",
      "code": "Aft",
      "label": "After",
      "description": "Reverse of Bef: left interval starts after right interval ends, within delta (threshold).",
      "arity": 2,
      "parameters": {
        "supported": ["delta", "rho", "zeta"],
        "defaults": { "delta": "∞", "rho": 0, "zeta": "≤" },
